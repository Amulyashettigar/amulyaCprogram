#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int num;
    scanf("%d", &num);
    if(num==6 )
     printf("1 2");
      else if(num==9 )
     printf("2 2");
     else if(num==1 )
     printf("0 0");
      else if(num==2 )
     printf("0 1");
     else if(num==3 )
     printf("0 2");
     else if(num==4 )
     printf("1 0");
      else if(num==5 )
     printf("1 1");
     else if(num==7 )
     printf("2 0");
     else if(num==8 )
     printf("2 1");
    
    return 0;
}
