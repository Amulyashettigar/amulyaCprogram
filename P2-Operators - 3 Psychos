#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    float w, x, y, z;
    float a, b;
    scanf("%f", &w);
    scanf("%f", &x);
    scanf("%f", &y);
    scanf("%f", &z);
    a = ( y + w)/2;
    b = (z + x)/2;
    printf("Arun's house is located at(%.1f,%.1f)", a, b);
    return 0;
}
