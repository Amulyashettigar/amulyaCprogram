#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
           char alpha;
    scanf("%c", &alpha);
    if(isalpha(alpha))
    {
    if (alpha == 'a' || alpha == 'e' || alpha == 'i' || alpha == 'o' || alpha == 'u' || alpha == 'A' || alpha == 'E' || alpha == 'I' || alpha == 'O' || alpha == 'U')
    {
    printf("Vowel\n");
    }
    else{
    printf("Consonant\n"); 
    }
    }
    else
    {
        printf("Not an alphabet");
    }
    
    return 0; 
}
