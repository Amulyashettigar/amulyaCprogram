#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int m,n,i, product;
    printf("Enter n\n");
    scanf("%d", &n);
    printf("Enter m\n");
    scanf("%d", &m);
    
    printf("The multiplication table of %d is\n", n);
    
    for(i=1; i<=m ; i++)
    { 
         product =i*n;
        printf("%d*%d=%d\n", i, n, product);
        
    }
    return 0;
}
